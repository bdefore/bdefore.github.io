import com.kelvinluck.flickr.*;import net.hiddenresource.util.Debug;class sspResponseListener extends FlickrResponseListener {	var phpFile:String = "userReq.php";	var extraParams:String;	var trg:MovieClip;		function sspResponseListener(trg:MovieClip) {		super();		this.trg = trg;	}		function onPeopleFindByUsername(p:Object) {		this.extraParams = "?user=" + p.username + "&id=" + p.nsid;		Debug.trace("In Listener: " + extraParams);		this.initSSP(extraParams);	}	function onPeopleFindByEmail(p:Object) {		this.extraParams = "?user=" + p.username + "&id=" + p.nsid;		Debug.trace("In Listener: " + extraParams);		this.initSSP(extraParams);	}		function initSSP(extraParams:String):Void {		var loadPath = "http://floccer.com/jimiflickr/" + this.phpFile + this.extraParams;		Debug.trace("loadXML: " + loadPath);		Debug.trace("pos: " + trg._x);		trg.xmlFilePath = loadPath;	}}